set terminal png
set output "$@"
set key outside
set tics in
set grid
set xlabel "No. of rows (cols), $n$"
set ylabel "Runtime, $t$"
set title "Running time of Jacobi decomposition"
set output "part_C_myMethod.png"
plot "my_timing.txt" using 1:2 with line lw 2 col MidnightBlue title "Runtime of my implementation"\
, "my_timing.txt" using 1:3 with line lw 2 col Black title "$\mathcal{O}(n^3)$"\
